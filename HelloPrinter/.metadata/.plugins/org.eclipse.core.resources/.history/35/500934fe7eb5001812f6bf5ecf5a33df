
public class AverageAgeDavis {
	// Spencer Davis
	public static void main(String[] args) {
		
		int keithAge = 50;
		int nathanAge = 28;
		int spencerAge = 25;
		int bresciaAge = 23;
		int elizaAge = 18;

		double avgAgeOfDavisFam = (keithAge + nathanAge + spencerAge + bresciaAge + elizaAge) / 5;
		
		System.out.println("The Davis family is an average of " + avgAgeOfDavisFam + " years old for the siblings!");
	}

}
