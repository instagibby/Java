
public class DataTypeDemoDavis {
	
	public static void main(String[] args) {
		
		int numOfCars = 1;
		System.out.println("I own "+ numOfCars + " car!");
		
		double carsPaidOff = 0.5;
		System.out.println("I only have paid off" + carsPaidOff + " of my car!");
		
		float carPercentToPayOff = 0.010f;
		boolean isCarPaidOff = false;
		short carLoanLeft = 400;
		long desiredBankAccount = 100000;
		byte monthlyCarPayment = 114;
		char a = 'A';

	}
}
