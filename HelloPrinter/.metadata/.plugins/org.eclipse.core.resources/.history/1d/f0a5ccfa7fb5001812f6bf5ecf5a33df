
public class AverageAgeDavis {
	// Spencer Davis
	public static void main(String[] args) {
		
		int keithAge = 30;
		int nathanAge = 28;
		int spencerAge = 25;
		int bresciaAge = 23;
		int elizaAge = 18;

		double avgAgeOfDavisFam = ((double)keithAge + (double)nathanAge + (double)spencerAge + (double)bresciaAge + (double)elizaAge) / 5;
		
		System.out.println("The Davis family siblings are an average of " + avgAgeOfDavisFam + " years old!");
	}

}
